{
  "git": {
    "deploymentEnabled": {
      "main": false
    }
  },
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "devCommand": "npm run start",
  "rewrites": [
    {
      "source": "/partners-area",
      "destination": "/middleware"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/404",
      "statusCode": 404
    }
  ]
}
